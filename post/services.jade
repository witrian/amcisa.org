//- 
  Similar layout as events.jade.

extends ./layout.jade

block title
  title Services

block content
  .panel.panel-info.section
    .panel-heading
      h1 All Services
    .panel-body
      .container-fluid
        .col-md-3
          span.fa.fa-share-alt-square.ico-large(style="color:rgb(237, 119, 119)")
          h3 Event Registration
        .col-md-3 
          span.fa.fa-binoculars.ico-large(style="color:rgb(260, 222, 109)")
          h3 Course Guide
        a(href="faq.html")
          .col-md-3
            span.fa.fa-question.ico-large(style="color:rgb(188, 223, 138)")
            h3 FAQ
        .col-md-3
          span.fa.fa-pencil-square.ico-large(style="color:rgb(148, 192, 204)")
          h3 Prospective Students
      